{
	"version": "1.1.23",
	"name": "@excali-boards/boards-api-client",
	"description": "A simple API client for the Boards API.",
	"repository": "https://github.com/Excali-Boards/boards-api-client",
	"author": "Digital39999",
	"scripts": {
		"ts": "pnpm install typescript --save-dev -g",
		"init": "pnpm install && pnpm run build",
		"build": "tsc --build",
		"watch": "tsc --watch",
		"lint": "eslint . --ext .ts",
		"postinstall": "prisma generate",
		"updates": "pnpm npm-check-updates -i --format group"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"license": "GPL-3.0",
	"files": [
		"dist",
		"prisma/schema",
		"prisma.config.ts",
		"README.md",
		"LICENSE"
	],
	"pnpm": {
		"onlyBuiltDependencies": [
			"@prisma/client",
			"@prisma/engines",
			"prisma"
		]
	},
	"devDependencies": {
		"@types/node": "20.14.13",
		"@typescript-eslint/eslint-plugin": "6.18.0",
		"@typescript-eslint/parser": "6.18.0",
		"eslint": "8.56.0",
		"typescript": "5.9.2"
	},
	"engines": {
		"node": ">=20.2.0"
	},
	"dependencies": {
		"@prisma/client": "6.16.2",
		"axios": "1.12.2",
		"prisma": "6.16.2",
		"ts-prisma": "1.3.3",
		"zod": "4.1.9"
	}
}